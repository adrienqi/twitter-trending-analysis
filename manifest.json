{
  "manifest_version": 3,
  "name": "tte",
  "version": "1.0",
  "description": "currently insert element into twitter DOM",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_idle",
      "all_frames": true,
      "js": ["scripts/content.js"]
    }
  ]
}
